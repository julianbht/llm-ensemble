{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "EnsembleResult",
  "description": "Placeholder schema for aggregate CLI output. TODO: Define based on ensemble aggregation strategy implementation.",
  "type": "object",
  "required": ["query_id", "docid", "final_label"],
  "properties": {
    "query_id": {
      "type": "string",
      "description": "Query ID"
    },
    "docid": {
      "type": "string",
      "description": "Document ID"
    },
    "final_label": {
      "type": "integer",
      "enum": [0, 1, 2],
      "description": "Ensemble aggregated label: 2=highly relevant, 1=relevant, 0=not relevant"
    },
    "final_confidence": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0,
      "description": "Aggregated confidence score"
    },
    "per_model_votes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "model_id": {"type": "string"},
          "label": {"type": ["integer", "null"]},
          "confidence": {"type": ["number", "null"]}
        }
      },
      "description": "Individual model votes used in aggregation"
    },
    "aggregation_strategy": {
      "type": "string",
      "description": "Name of aggregation strategy used (e.g., 'weighted_majority', 'unanimous')"
    },
    "warnings": {
      "type": "array",
      "items": {"type": "string"},
      "default": [],
      "description": "Warnings from aggregation (e.g., ties, low agreement)"
    }
  }
}
